<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- webpage - name of the web page diesplay when the app starts  -->
    <!-- webpage - if value none then no web page is displayed at start uo  -->
    <!-- add key="webpage" value="http://www.youtube.com/watch?v=I_izvAbhExY"  BeGee's Song-->
    <!-- add key="webpage" value="http://www.youtube.com/watch?v=I_izvAbhExY" -->
    <!-- webpage - name of the web page diesplay when the app starts  -->
    <add key="webpage" value="http://www.boeing.com" />
    <!-- mouseClickInterval - number of seconds between middle mouse button clicks -->
    <add key="mouseClickInterval" value="60"/>
    <!-- timeForm - values - std,mil, an explicit format hh:mm tt -->
    <add key="timeFormat" value="std"/>
    <!-- linkLabel text to display for the hyperlink -->
    <add key="linkLabel" value="ETS"/>
    <!-- hyperLink the http location for the link -->
    <add key="hyperLink" value="https://ets-prd.web.boeing.com"/>
    <!-- quitTime - the normal military time hour that you quit working 
    if this does not exist, then the etsWebpage won't be displayed -->
    <add key="quitTime" value="17"/>
    <add key="startTime" value="8"/>
    <add key="noClickTime" value="18"/>
    <!-- etsWebpage - the name of the ETS webpage to display at quitTime  -->
    <add key="etsWebpage" value="https://ets-prd.web.boeing.com/Record%20Daily%20Time.aspx"/>
    <!-- etsAlert sound to play when etsWebpage is displayed -->
    <add key="etsAlert" value="C:\Users\zf297a\Music\sounds\to-arms-bugle-call.wav"/>
    <!-- browser used to display web pages -->
    <add key="browser" value="C:\Program Files (x86)\Microsoft\Edge\Application\msedge.exe"/>
  </appSettings>
</configuration>
